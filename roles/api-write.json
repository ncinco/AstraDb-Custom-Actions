{
    "name": "Api Write __DOMAIN__",
    "policy": {
        "description": "Role for read and write operations to tables.",
        "resources": [
            "drn:astra:org:__ASTRA_ORG_ID__:db:__ASTRA_DB_ID__:keyspace:*"
        ],
        "actions": [
            "db-table-select",
            "db-table-modify",
            "db-cql",
            "db-rest"
        ],
        "effect": "allow"
    }
}
